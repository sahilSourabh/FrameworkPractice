<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="false">


	<groups>
		<run>
			<include name="ErrorHandling" />
		</run>
	</groups>

	<listeners>
		<listener class-name="testingframework.TestComponents.Listeners" />
	</listeners>


	<test name="Submit Order Test" thread-count="5">
		<classes>
			<class name="testingframework.Tests.SubmitOrderTest" />
		</classes>
	</test> <!--
	Test -->

	<test name="Error Validation Test">
		<classes>
			<class name="testingframework.Tests.ErrorValidationsTest" />
		</classes>
	</test>

</suite> <!--
Suite -->
